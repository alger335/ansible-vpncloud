---
device:
  type: {{ vpncloud_vpn_type }}
  name: {{ vpncloud_netname }}%d
  path: ~
  fix-rp-filter: false
ip: {{ vpncloud_ip }}
ifup: ~
ifdown: ~
crypto:
  password: PaSSw0rD
  private-key: {{ vpncloud_privkey }}
  public-key: {{ vpncloud_pubkey }}
  trusted-keys: []
  algorithms: []
listen: "3210"
peers: []
peer-timeout: 300
keepalive: ~
beacon:
  store: ~
  load: ~
  interval: 3600
  password: ~
mode: normal
switch-timeout: 300
claims: []
auto-claim: true
port-forwarding: true
pid-file: ~
stats-file: ~
statsd:
  server: ~
  prefix: ~
user: ~
group: ~
hook: ~
hooks: {}

